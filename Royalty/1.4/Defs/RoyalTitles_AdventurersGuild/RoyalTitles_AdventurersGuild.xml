  <!-- ============== Empire titles - Player gettable non-psycaster ============================= -->

  <RoyalTitleDef Abstract="True" Name="EEG_AdventurerGuildTitleBase" >
    <tags>
      <li>AdventurerGuildRanks</li>
    </tags>
    <awardThought>GainedTitle</awardThought>
    <lostThought>LostTitle</lostThought>
  </RoyalTitleDef>

  <RoyalTitleDef ParentName="EEG_AdventurerGuildTitleBase">
    <defName>EEG_RT1_Copper</defName>
    <label>copper rank adventurer</label>
    <description>Copper rank adventurers are new members of the guild who are still learning the ropes. They are given basic training in combat and are expected to follow the orders of their superiors.</description>
    <seniority>0</seniority>
    <favorCost>1</favorCost>
    <changeHeirQuestPoints>1000</changeHeirQuestPoints>
    <allowDignifiedMeditationFocus>False</allowDignifiedMeditationFocus>
    <!--awardWorkerClass>RoyalTitleAwardWorker_Instant</awardWorkerClass-->
  </RoyalTitleDef>

  <!-- ============== Empire titles - Player gettable psycaster ============================= -->

  <RoyalTitleDef ParentName="EEG_AdventurerGuildTitleBase">
    <defName>EEG_RT2_Bronze</defName>
    <label>bronze rank adventurer</label>
    <description>Bronze rank adventurers are slightly more experienced members of the guild who have demonstrated the ability to handle more responsibility. They may be given more complex tasks and may be allowed to participate in more advanced training.</description>
    <seniority>100</seniority>
    <favorCost>3</favorCost>
    <changeHeirQuestPoints>1000</changeHeirQuestPoints>
    <commonality>1</commonality>
    <!-- <maxPsylinkLevel>1</maxPsylinkLevel> -->
  </RoyalTitleDef>

